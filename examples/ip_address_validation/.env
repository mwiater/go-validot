# IP Address Validation Example .env

# API Configuration
API_KEY="12345abcdef"
API_URL="https://api.myapp.com/v1/"
API_SECRET="secretvalue123"
API_TIMEOUT="30"

# Service Configuration
SERVICE_ENDPOINT="https://service.myapp.com/endpoint"
SERVICE_VERSION="v2"

# Database Configuration
DB_HOST="localhost"
DB_PORT="5432"
DB_USER="admin"
DB_PASSWORD="securepassword"
DB_NAME="myapp_db"

# Environment Settings
ENVIRONMENT="STAGING"

# Feature Flags
ENABLE_DEBUG="true"
ENABLE_FEATURE_X="false"
ENABLE_FEATURE_Y="yes"
FEATURE_FLAG_NEW_UI="no"

# Network Configuration
TRUSTED_PROXY_IP="192.168.1.100"
DATABASE_IP="10.0.0.5"
REDIS_HOST="redis.local"
REDIS_PORT="6379"

# Logging Configuration
LOG_LEVEL="INFO"
LOG_FORMAT="json"

# Miscellaneous
SERVICE_TIMEOUT="60"
CACHE_SIZE="256"
UPLOAD_LIMIT="1048576"
USE_SSL="yes"
